{
    "name": "@anglr/error-handling",
    "version": "0.0.0-placeholder",
    "description": "Angular module used for handling http api error handling",
    "type": "module",
    "sideEffects": false,
    "scripts": {
        "build": "ngc --declaration --declarationDir . --declarationMap && ngc -t es2015 --outDir es2015 && npm run dts:rollup",
        "dts:rollup": "npm run api -- api-extractor.json && npm run api -- date-fns/api-extractor.json && npm run api -- forms/api-extractor.json && npm run api -- hmr/api-extractor.json && npm run api -- hotkeys/api-extractor.json && npm run api -- material/api-extractor.json && npm run api -- moment/api-extractor.json && npm run api -- numeral/api-extractor.json && npm run api -- positions/api-extractor.json && npm run api -- router/api-extractor.json && npm run api -- store/api-extractor.json && npm run api -- structured-log/api-extractor.json",
        "api": "api-extractor run --local --verbose -c",
        "clean": "rimraf */temp temp es2015 es2020 **/tsdoc-metadata.json src/**/*.d.ts src/**/*.d.ts.map */src/**/*.d.ts */src/**/*.d.ts.map ./*.d.ts ./*.d.ts.map */*.d.ts",
        "clean:deps": "rimraf package-lock.json node_modules",
        "clean:all": "npm run clean && npm run clean:deps",
        "ngc": "ngc",
        "version:placeholder": "replace-in-file /\\\"version\\\":\\s\\\".*?\\\"/g \"\\\"version\\\": \\\"0.0.0-placeholder\\\"\" package.json --isRegex",
        "version:git": "ngv -e \"replace-in-file 0.0.0-placeholder $GIT_VERSION package.json && echo $GIT_VERSION > version.bak\"",
        "version:build": "ngv -e \"echo $GIT_VERSION > version.bak\"",
        "prepublishOnly": "npm run bundle && npm run version:placeholder && npm run version:git",
        "postpublish": "npm run doc && npm run version:placeholder && npm run clean",
        "bundle": "npm run clean && npm run build",
        "deploy": "npm publish --loglevel error",
        "deploy:next": "npm publish --tag next --loglevel error",
        "lint": "eslint src/**/*.ts",
        "doc": "path-exists docs && npm run doc:generate || echo \"No docs generated!\"",
        "predoc:generate": "cd docs && git pull && cd ..",
        "doc:generate": "npm run doc:generate:error-handling && npm run doc:generate:html2canvas && npm run doc:generate:material && npm run doc:save",
        "doc:save": "cd docs && git add . && git commit -m \"INT: updated API docs ng-error-handling\" && git push && cd ..",
        "doc:generate:error-handling": "rimraf docs/content/api/ng-error-handling && npm run doc:json:error-handling && npm run doc:md:error-handling && npm run doc:api:error-handling",
        "doc:json:error-handling": "api-extractor run --local --verbose -c api-extractor.json",
        "doc:md:error-handling": "api-documenter markdown -i temp -o docs/content/api/ng-error-handling",
        "doc:api:common": "copyfiles -u 1 temp/error-handling.api.md docs/content/api/ng-error-handling && rimraf temp",
        "doc:generate:html2canvas": "rimraf docs/content/api/ng-error-handling-html2canvas && npm run doc:json:html2canvas && npm run doc:md:html2canvas && npm run doc:api:html2canvas",
        "doc:json:html2canvas": "api-extractor run --local --verbose -c html2canvas/api-extractor.json",
        "doc:md:html2canvas": "api-documenter markdown -i html2canvas/temp -o docs/content/api/ng-error-handling-html2canvas && rimraf html2canvas/temp",
        "doc:api:html2canvas": "copyfiles -u 2 hmr/temp/error-handling-html2canvas.api.md docs/content/api/ng-error-handling-html2canvas && rimraf html2canvas/temp",
        "doc:generate:material": "rimraf docs/content/api/ng-error-handling-material && npm run doc:json:material && npm run doc:md:material",
        "doc:json:material": "api-extractor run --local --verbose -c material/api-extractor.json",
        "doc:md:material": "api-documenter markdown -i material/temp -o docs/content/api/ng-error-handling-material && rimraf material/temp"
    },
    "engines": {
        "node": "^12.20.0 || ^14.15.0 || >=16.10.0"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ressurectit/ng-error-handling.git"
    },
    "keywords": [
        "angular",
        "ng",
        "error",
        "handling"
    ],
    "author": "kukjevov",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/ressurectit/ng-error-handling/issues"
    },
    "homepage": "https://github.com/ressurectit/ng-error-handling",
    "peerDependencies": {
        "@angular/core": ">=13.1.0",
        "@angular/common": ">=13.1.0",
        "@angular/platform-browser": ">=13.1.0",
        "@angular/forms": ">=13.1.0",
        "@angular/cdk": ">=13.1.0",
        "@angular/material": ">=13.1.0",
        "rxjs": ">=6.6.7",
        "@jscrpt/common": ">=2.2.0",
        "@anglr/common": ">=8.0.0",
        "html2canvas": "^1.3.3",
        "sourcemapped-stacktrace": "1.1.11",
        "tslib": "^2.3.1"
    },
    "devDependencies": {
        "@angular/core": "^13.1.0",
        "@angular/common": "^13.1.0",
        "@angular/platform-browser": "^13.1.0",
        "@angular/forms": "^13.1.0",
        "@angular/animations": "^13.1.0",
        "@angular/cdk": "^13.1.0",
        "@angular/material": "^13.1.0",
        "rxjs": "^6.6.7",
        "@jscrpt/common": "^2.2.0",
        "@anglr/common": "8.0.0",
        "html2canvas": "^1.3.3",
        "@types/node": "^14.14.31",
        "@angular/compiler-cli": "^13.1.0",
        "@angular/compiler": "^13.1.0",
        "@angular/language-service": "^13.1.0",
        "@microsoft/api-extractor": "7.19.2",
        "@microsoft/api-documenter": "7.13.77",
        "tslib": "2.3.1",
        "eslint": "7.32.0",
        "@typescript-eslint/eslint-plugin": "4.33.0",
        "@typescript-eslint/parser": "4.33.0",
        "eslint-plugin-ressurectit": "0.1.0",
        "rimraf": "3.0.2",
        "copyfiles": "2.4.1",
        "replace-in-file": "6.3.2",
        "path-exists-cli": "2.0.0",
        "typescript": "4.5.4",
        "npm-git-version": "2.0.0"
    },
    "esm2020": "./es2020/src/index.js",
    "es2020": "./es2020/src/index.js",
    "es2015": "./es2015/src/index.js",
    "module": "./es2015/src/index.js",
    "typings": "./error-handling.d.ts",
    "exports": {
        "./package.json": {
            "default": "./package.json"
        },
        ".": {
            "types": "./error-handling.d.ts",
            "esm2020": "./es2020/src/index.js",
            "es2020": "./es2020/src/index.js",
            "es2015": "./es2015/src/index.js",
            "node": "./es2015/src/index.js",
            "default": "./es2020/src/index.js"
        },
        "./date-fns": {
            "types": "./date-fns/error-handling-date-fns.d.ts",
            "esm2020": "./es2020/date-fns/src/index.js",
            "es2020": "./es2020/date-fns/src/index.js",
            "es2015": "./es2015/date-fns/src/index.js",
            "node": "./es2015/date-fns/src/index.js",
            "default": "./es2020/date-fns/src/index.js"
        }
    }
}
