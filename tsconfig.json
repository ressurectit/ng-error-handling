{
    "compilerOptions":
    {
        "target": "es5",
        "module": "commonjs",
        "sourceMap": false,
        "declaration": true,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "moduleResolution": "node",
        "outDir": "dist",
        "lib": 
        [
            "es6", "dom"
        ],
        "types": 
        [
            "jquery",
            "html2canvas"
        ],
        "typeRoots": 
        [
            "node_modules/@ng2/types"
        ]
    },
    "exclude": 
    [
        "node_modules"
    ]
}